{
  "name": "interactv",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.2",
  "scripts": {
    "dev": "concurrently \"cd frontend && bun dev\" \"cd backend && bun dev\"",
    "dev:frontend": "cd frontend && bun dev",
    "dev:backend": "cd backend && bun dev",
    "build": "cd frontend && bun run build",
    "start": "cd frontend && bun start",
    "lint": "cd frontend && bun lint"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
